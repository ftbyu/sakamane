<div class="row">
  <div class="col-md-6">
    <div class="text-center">
      <h1><%= "画像 vs 画像" %></h1><br>
      <h1><%= "#{score(@game)} : #{@game.enemy_score}" %></h1><br>
      <h3><%= "日付 #{@game.created_at}" %></h3><br>
      <h3><%= "場所 #{@game.location.name}" %></h3><br>

      <%# Youtube動画 %>
    </div>
  </div>
  <div class="col-md-6">
    <div class="text-center">
      <table class="table table-striped">
        <thead>
            <tr>
                <th>項目</th>
                <th>総数</th>
                <th>成功率</th>
            </tr>
        </thead>
        <tbody>
          <% @analiyses.each do |analiysis| %>
            <tr>
              <td><%= analiysis.name %></td>
              <td><%= total(@game, analiysis) %></td>
              <td></td>
            </tr>
          <% end %>
        </tbody> 
      </table>
    </div>
  </div>
</div>