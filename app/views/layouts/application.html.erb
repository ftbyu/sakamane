<!DOCTYPE html>
<html lang="en">
  <head>
    <title>SB Admin 2 - Dashboard</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="https://use.fontawesome.com/releases/v5.6.1/css/all.css" rel="stylesheet">
  </head>

  <body id="page-top">

    <div id="wrapper">

      <%= render 'layouts/sidebar' %>

      <!-- Content Wrapper -->
      <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
          <%= render 'layouts/topbar' %>
          <div class="container-fluid">
            <%= yield %>
          </div>
        </div>

        <%= render 'layouts/footer' %>
      </div>

    </div>

    <!-- Scroll to Top Button-->
    <a class="scroll-to-top rounded" href="#page-top">
        <i class="fas fa-angle-up"></i>
    </a>

    <!-- Bootstrap core JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js"></script>
    <!-- Third party plugin JS-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
    
    <!-- Bootstrap core JavaScript-->
    <%= javascript_include_tag('jquery.min.js') %>
    <%= javascript_include_tag('bootstrap.bundle.min.js') %>

    <!-- Core plugin JavaScript-->
    <%= javascript_include_tag('jquery.easing.min.js') %>

    <!-- Custom scripts for all pages-->
    <%= javascript_include_tag('sb-admin-2.js') %>

    <!-- Page level plugins -->
    <%= javascript_include_tag('Chart.min.js') %>

    <!-- Page level custom scripts -->
    <%= javascript_include_tag('chart-area-demo.js') %>
    <%= javascript_include_tag('chart-pie-demo.js') %>

  </body>
</html>
